# Hub basic auth (optional)
DAILY_APP_LAB_USER=ming
DAILY_APP_LAB_PASS=

# Hub port (default 41777)
DAILY_APP_LAB_PORT=41777

# Engine root override (optional; by default hub will try to locate packages/engine)
DAILY_APP_LAB_ROOT=

# Model selection (e.g., azure/gpt-4o, openai/gpt-4-turbo)
DAILY_APP_LAB_MODEL=azure/gpt-5.2

# Aider binary path (optional, default: aider)
DAILY_APP_LAB_AIDER_BIN=aider

# RAG similarity threshold (optional, default: 0.72)
DAILY_APP_LAB_RAG_SIM_THRESHOLD=0.72

# Clawdbot config path (optional)
CLAWDBOT_CONFIG=

# If you use a proxy for app research (optional)
HTTPS_PROXY=
HTTP_PROXY=
